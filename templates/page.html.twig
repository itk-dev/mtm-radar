<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}{{ site_name }}{% endblock %}</title>
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous"/>
        <link rel="stylesheet" href="{{ asset('assets/dist/css/page.css') }}"/>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            {% block header %}
                <header>
                    <nav class="navbar navbar-expand-md fixed-top">
                        <div class="container">
                            <a class="navbar-left" href="/">
                                <img src="{{ asset(site_logo_url) }}"/>
                            </a>
                        </div>
                    </nav>
                </header>
            {% endblock %}

            <main role="main" class="container">
                {% block content %}{% endblock %}
            </main>

            {% block footer %}
                <footer class="footer">
                    <div class="container">
                        <h1> container page fra page.html.twig </h1>
                        {# {% if craue_setting('page_footer') %}
                            {{ craue_setting('page_footer') }}
                        {% else %}
                            {% set url = path('easyadmin', {entity: 'Setting', action: 'edit', 'id': 'page_footer'}) %}
                            Go to <a href="{{ url }}">{{ url }}</a> and edit the page footer.
                        {% endif %} #}
                    </div>
                </footer>
            {% endblock %}
        {% endblock %}

        <script src="{{ asset('assets/dist/js/jquery.min.js') }}"></script>
        <script src="{{ asset('assets/dist/js/popper.min.js') }}"></script>
        <script src="{{ asset('assets/dist/js/bootstrap.min.js') }}"></script>

        {% block javascripts %}{% endblock %}
    </body>
</html>
